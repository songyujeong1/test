{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "stage-2"
  ],
  "plugins": ["transform-vue-jsx", "transform-runtime"],
  "env": {
    "staging": {
      "plugins": [
        [
          "transform-remove-console", { "exclude": ["info", "error"] }
        ]
      ]
    },
    "production": {
      "plugins": [
        [
          "transform-remove-console"
        ]
      ]
    }
  }
}
